{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
      },
      "eslint.alwaysShowStatus": true,
      "eslint.format.enable": true,
      "eslint.run": "onSave",
      "eslint.validate": [
        "typescript"
      ],
      "eslint.workingDirectories": [
        {
          "mode": "auto"
        }
      ]
}
